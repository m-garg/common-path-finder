{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "no-console":[
            "off"
        ],
        "no-unused-vars":[
            "warn"
        ]
    }
}